{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": false,
        "no-trailing-whitespace": false,
        "no-console": false,
        "eofline": false,
        "object-literal-sort-keys": false,
        "one-line": [
            true,
            "check-catch",
            "check-finally",
            "check-open-brace",
            "check-whitespace"
        ],
        "ordered-imports": false,
        "arrow-parens": false,
        "comment-format": false,
        "no-var-requires": false,
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "objects": "always",
                    "arrays": "always",
                    "functions": "never",
                    "typeLiterals": "ignore"
               },
               "esSpecCompliant": true
            }
        ],
        "forin": false,
        "interface-name": false
    },
    "rulesDirectory": []
}